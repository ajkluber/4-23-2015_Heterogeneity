title = {{Universality in the timescales of internal loop formation in unfolded proteins and single-stranded oligonucleotides.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3000509\&tool=pmcentrez\&rendertype=abstract},
volume = {99},
year = {2010}
}
