month = sep,
number = {12},
pages = {2699--706},
pmid = {20589630},
title = {{On the pH-optimum of activity and stability of proteins.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2911520\&tool=pmcentrez\&rendertype=abstract},
volume = {78},
