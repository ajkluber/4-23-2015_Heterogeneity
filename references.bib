pmid = {9600886},
title = {{A statistical mechanical model for beta-hairpin kinetics.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=34489\&tool=pmcentrez\&rendertype=abstract},
volume = {95},
year = {1998}
}
@article{Munoz1996,
abstract = {One of the classic important issues in protein folding and stability is the relative roles of noncovalent short-range (local) and long-range (nonlocal) interactions. Interest in this topic has been reinforced by recent developments in the analytical theory of protein folding and in lattice-based computer simulations. During the past few years, a wealth of experimental information relevant to this issue has been accumulating. In this review, we focus specifically on experimental aspects, discussing some general ideas that arise from the results obtained by many different groups using a variety of approaches. We also discuss a new experimental strategy that allows us to engineer the contribution of local interactions, and we discuss the first results obtained.},
author = {Mu\~{n}oz, V and Serrano, L},
