keywords = {Amino Acid Sequence,Aprotinin,Aprotinin: chemistry,Hydrogen Bonding,Molecular Sequence Data,Plant Proteins,Plant Proteins: chemistry,Protein Structure, Secondary,Proteins,Proteins: chemistry,Solubility,Thermodynamics},
month = mar,
