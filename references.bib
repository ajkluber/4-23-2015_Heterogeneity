keywords = {Amyloid,Amyloid: chemistry,Amyloid: metabolism,Amyloid: ultrastructure,Animals,Atomic Force,Cattle,Insulin,Insulin: chemistry,Insulin: metabolism,Kinetics,Microscopy,Nanostructures,Nanostructures: chemistry,Nanostructures: ultrastructure,Protein Binding,experiment,insulin},
