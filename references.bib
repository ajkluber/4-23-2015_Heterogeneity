journal = {Nat. Struct. Biol.},
keywords = {Computer Simulation,Protein Denaturation,Protein Denaturation: physiology,Protein Structure, Secondary,Protein Structure, Secondary: physiology,Staphylococcal Protein A,Staphylococcal Protein A: metabolism},
month = nov,
number = {11},
