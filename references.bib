pages = {5283--8},
pmid = {22421432},
title = {{Direct observation of multiple misfolding pathways in a single prion protein molecule}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3325692\&tool=pmcentrez\&rendertype=abstract http://www.pnas.org/content/109/14/5283.short},
