keywords = {DNA,DNA: chemistry,Data Interpretation, Statistical,Laboratories,Thermodynamics,Uncertainty},
month = sep,
number = {12},
pages = {124105},
pmid = {19045004},
title = {{Statistically optimal analysis of samples from multiple equilibrium states.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2671659\&tool=pmcentrez\&rendertype=abstract},
volume = {129},
year = {2008}
}
