journal = {Biomacromolecules},
keywords = {Hydrogel,Hydrogel: chemistry,Hydrogen-Ion Concentration,Mutagenesis, Site-Directed,Peptides,Peptides: chemistry,Phase Transition,Protein Structure, Secondary,Rheology,Static Electricity,Transition Temperature},
month = sep,
number = {9},
pages = {2619--25},
pmid = {19663418},
title = {{Tuning the pH responsiveness of beta-hairpin peptide folding, self-assembly, and hydrogel material formation.}},
