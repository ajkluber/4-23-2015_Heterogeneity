journal = {Proc. Natl. Acad. Sci.},
keywords = {Computer Simulation,Models, Chemical,Mutagenesis,Nerve Tissue Proteins,Nerve Tissue Proteins: chemistry,Nerve Tissue Proteins: genetics,PDZ Domains,Protein Folding,Protein Tyrosine Phosphatase, Non-Receptor Type 13,Thermodynamics},
month = dec,
number = {49},
pages = {19241--6},
pmid = {19033470},
