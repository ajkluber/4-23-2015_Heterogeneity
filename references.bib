issn = {0002-7863},
journal = {J. Am. Chem. Soc.},
month = apr,
number = {13},
pages = {4466--4474},
title = {{De Novo Design of Strand-Swapped $\beta$-Hairpin Hydrogels}},
url = {http://dx.doi.org/10.1021/ja710295t http://pubs.acs.org/doi/full/10.1021/ja710295t},
volume = {130},
