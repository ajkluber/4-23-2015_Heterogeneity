issn = {0027-8424},
journal = {Proc. Natl. Acad. Sci.},
keywords = {Animals,Binding Sites,Cattle,Cell Membrane,Cell Membrane: metabolism,Crystallography, X-Ray,Crystallography, X-Ray: methods,Hydrogen Bonding,Ligands,Light,Models, Chemical,Models, Molecular,Protein Binding,Rhodopsin,Rhodopsin: chemistry,Rhodopsin: metabolism,Water,Water: chemistry},
month = apr,
