issn = {0027-8424},
journal = {Proc. Natl. Acad. Sci.},
keywords = {Biophysical Phenomena,Biophysics,Carrier Proteins,Carrier Proteins: chemistry,Carrier Proteins: genetics,Escherichia coli Proteins,Escherichia coli Proteins: chemistry,Escherichia coli Proteins: genetics,Fluorescence,Kinetics,Models, Molecular,Mutation,Mutation: genetics,Protein Conformation,Protein Folding,Spectrum Analysis, Raman},
month = jan,
number = {1},
pages = {99--104},
pmid = {16371468},
