volume = {17},
year = {2009}
}
@misc{TEDxTalks2011,
abstract = {Alice Walker is world renown for her fiction, poetry, essays, and human rights activism. Her fiction, in particular her novels, have established her as a canonical figure in American letters, as well as a major figure in what scholars term the renaissance in African American women's writings of the 1970s. Her writings have been translated into more than two dozen languages, and her books have sold more than fifteen million copies. In 1982 Alice published The Color Purple, now a classic of American literature, for which she was awarded the 1983 Pulitzer Prize for Fiction (the first African American woman writer to receive this award) and the National Book Award. The award-winning novel served as the inspiration for Steven Spielberg's 1985 film and has been adapted for the stage by Marsha Norman and produced by Scott Sanders. 

