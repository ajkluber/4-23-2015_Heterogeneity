keywords = {Allosteric Regulation,Allosteric Regulation: physiology,Kinetics,Mutation,Protein Conformation,Receptors, Cholinergic,Receptors, Cholinergic: chemistry,Receptors, Cholinergic: genetics,Receptors, Cholinergic: metabolism,Time Factors},
month = jan,
