issn = {1542-0086},
journal = {Biophys. J.},
keywords = {Aptamers, Nucleotide,Aptamers, Nucleotide: chemistry,Aptamers, Nucleotide: metabolism,Computer Simulation,Models, Molecular,Nucleic Acid Conformation,RNA, Messenger,RNA, Messenger: chemistry,RNA, Messenger: metabolism,S-Adenosylmethionine,S-Adenosylmethionine: metabolism,Thermodynamics,Untranslated Regions,Untranslated Regions: chemistry,Untranslated Regions: metabolism},
month = jan,
number = {2},
pages = {L7--9},
pmid = {19167285},
title = {{Nonlocal helix formation is key to understanding S-adenosylmethionine-1 riboswitch function.}},
