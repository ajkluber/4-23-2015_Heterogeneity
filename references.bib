isbn = {0022-2836 (Print)$\backslash$r0022-2836 (Linking)},
issn = {00222836},
journal = {J. Mol. Biol.},
keywords = {molecular,protein engineering,protein-protein interaction},
pages = {478--486},
pmid = {7739054},
title = {{Energetics of protein-protein interactions: analysis of the barnase-barstar interface by single mutations and double mutant cycles.}},
volume = {248},
year = {1995}
}
