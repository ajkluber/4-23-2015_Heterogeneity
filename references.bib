journal = {Proteins},
keywords = {Animals,Computer Simulation,Cricetinae,Mesocricetus,Models, Chemical,Peptide Fragments,Peptide Fragments: chemistry,Prions,Prions: chemistry,Protein Folding,Protein Structure, Secondary,Software,Solvents,Time Factors,Water},
