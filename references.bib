keywords = {Actinin,Actinin: chemistry,Actinin: metabolism,Amino Acid Sequence,Animals,Cytoskeletal Proteins,Cytoskeletal Proteins: genetics,Protein Structure, Secondary,Protein Subunits,Repetitive Sequences, Amino Acid,Spectrin,Spectrin: chemistry,Spectrin: metabolism},
month = feb,
