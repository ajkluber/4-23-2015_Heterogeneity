keywords = {Animals,Carbon Monoxide,Carbon Monoxide: metabolism,Humans,Kinetics,Models, Molecular,Motion,Myoglobin,Myoglobin: chemistry,Myoglobin: metabolism,Protein Conformation,Proteins,Proteins: chemistry,Solvents,Temperature,Viscosity,Water,Water: chemistry},
month = mar,
number = {13},
pages = {5129--34},
pmid = {19251640},
title = {{A unified model of protein dynamics.}},
