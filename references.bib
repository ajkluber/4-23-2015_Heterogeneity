journal = {Proteins},
keywords = {Amino Acid Motifs,Databases, Factual,Hydrogen Bonding,Models, Molecular,Protein Folding,Protein Structure, Secondary,Protein Structure, Tertiary,Sequence Analysis, Protein},
month = aug,
number = {3},
pages = {409--19},
pmid = {10861931},
title = {{A comprehensive analysis of the Greek key motifs in protein beta-barrels and beta-sandwiches.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10861931},
volume = {40},
year = {2000}
}
