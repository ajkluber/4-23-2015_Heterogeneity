keywords = {Desiccation,Hydrophobic and Hydrophilic Interactions,Ion Channel Gating,Ion Channels,Ion Channels: metabolism,Molecular Dynamics Simulation,Protein Structure, Secondary,Protein Structure, Tertiary,Thermodynamics},
month = jul,
number = {2},
pages = {219--27},
pmid = {22853899},
publisher = {Biophysical Society},
title = {{Drying transition in the hydrophobic gate of the GLIC channel blocks ion conduction.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3400787\&tool=pmcentrez\&rendertype=abstract},
