keywords = {Algorithms,Aquaporins,Aquaporins: chemistry,Cell Membrane,Cell Membrane: chemistry,Computer Simulation,Glycophorin,Glycophorin: chemistry,Models, Biological,Models, Chemical,Models, Molecular,Repressor Proteins,Repressor Proteins: chemistry,Software,Software Design,Static Electricity,Ubiquitin,Ubiquitin: chemistry},
