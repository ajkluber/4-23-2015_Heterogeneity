pmid = {7937758},
title = {{Statistical mechanics of kinetic proofreading in protein folding in vivo.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=44798\&tool=pmcentrez\&rendertype=abstract},
volume = {91},
year = {1994}
}
