month = oct,
number = {10},
pages = {1645--58},
pmid = {21826755},
title = {{Pre-existing soft modes of motion uniquely defined by native contact topology facilitate ligand binding to proteins.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3218357\&tool=pmcentrez\&rendertype=abstract},
