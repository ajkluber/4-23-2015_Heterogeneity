keywords = {Acclimatization,Acclimatization: physiology,Animals,Antifreeze Proteins,Antifreeze Proteins: chemistry,Antifreeze Proteins: genetics,Antifreeze Proteins: metabolism,Freezing,Insects,Insects: chemistry,Insects: genetics,Insects: metabolism,Protein Conformation},
