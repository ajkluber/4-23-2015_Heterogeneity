issn = {0022-2836},
journal = {J. Mol. Biol.},
keywords = {Models, Molecular,Nuclear Magnetic Resonance, Biomolecular,Protein-Tyrosine Kinases,Protein-Tyrosine Kinases: chemistry,Solvents,Solvents: chemistry,Spectrin,Spectrin: chemistry,src Homology Domains},
month = jun,
