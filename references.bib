journal = {Protein Eng. Des. Sel.},
keywords = {Chevron plots,Crystallography,Interaction energies,Kinetics,Mutants},
number = {11},
pages = {547--557},
pmid = {16230299},
title = {{Antagonism, non-native interactions and non-two-state folding in S6 revealed by double-mutant cycle analysis}},
volume = {18},
year = {2005}
}
