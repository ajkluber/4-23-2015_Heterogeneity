journal = {Proc. Natl. Acad. Sci.},
keywords = {Membranes, Artificial,Models, Molecular,Nanotechnology,Nanotubes, Carbon,Osmosis,Solutions,Solutions: chemistry,Thermodynamics,Water,Water: chemistry},
month = sep,
number = {18},
pages = {10175--80},
pmid = {12878724},
title = {{Osmotic water transport through carbon nanotube membranes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=193535\&tool=pmcentrez\&rendertype=abstract},
volume = {100},
year = {2003}
}
