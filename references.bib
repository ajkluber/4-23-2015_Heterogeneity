file = {:home/alex/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Marchut, Hall - 2006 - Spontaneous formation of annular structures observed in molecular dynamics simulations of polyglutamine peptides.pdf:pdf},
