issn = {1089-8638},
journal = {J. Mol. Biol.},
keywords = {Animals,Binding Sites,Calcium,Calcium: metabolism,Circular Dichroism,Dogs,Goats,Kinetics,Lactalbumin,Lactalbumin: chemistry,Lactalbumin: metabolism,Milk,Milk: enzymology,Models, Molecular,Muramidase,Muramidase: chemistry,Muramidase: metabolism,Nuclear Magnetic Resonance, Biomolecular,Protein Conformation,Protein Folding,Protein Interaction Domains and Motifs,Protein Interaction Mapping,Protein Stability,Recombinant Proteins,Recombinant Proteins: chemistry,Recombinant Proteins: metabolism,Structural Homology, Protein},
