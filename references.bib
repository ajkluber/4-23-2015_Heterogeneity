file = {:home/alex/Work/Reading/All/He\_Chen\_Bryan\_Orban\_Mutational\_tipping\_points\_for\_switching\_protein\_folds\_and\_functions\_2012.pdf:pdf},
isbn = {0969-2126},
issn = {09692126},
journal = {Structure},
