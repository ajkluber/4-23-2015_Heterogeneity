issn = {1364-503X},
journal = {Philos. Trans. A. Math. Phys. Eng. Sci.},
keywords = {Computer Simulation,Energy Transfer,Models, Chemical,Models, Molecular,Multiprotein Complexes,Multiprotein Complexes: chemistry,Phase Transition,Protein Conformation,Protein Folding,Proteins,Proteins: chemistry,Temperature},
month = feb,
number = {1827},
pages = {433--50; discussion 450--2},
pmid = {15664892},
title = {{Towards complete descriptions of the free-energy landscapes of proteins.}},
