doi = {10.1038/nsb995},
file = {:home/alex/Work/Reading/All/Zagrovic\_Pande\_Structural\_correspondence\_between\_the\_alpha\_helix\_and\_the\_random-flight\_chain\_resolves\_how\_unfolded\_proteins\_can\_have\_native-like\_properties\_2003.pdf:pdf},
