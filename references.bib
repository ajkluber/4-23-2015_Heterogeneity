doi = {10.1006/jmbi.2000.3778},
file = {:home/alex/Work/Reading/All/Bateman\_Bycroft\_The\_structure\_of\_a\_LysM\_domain\_from\_E\_coli\_membrane-bound\_lytic\_murein\_transglycosylatse\_D\_2000.pdf:pdf},
