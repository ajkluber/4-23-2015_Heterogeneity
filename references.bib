journal = {Proc. Natl. Acad. Sci.},
keywords = {Biophysics,Biophysics: methods,Computer Simulation,Diffusion,Electrons,Fluorescence Resonance Energy Transfer,Markov Chains,Models, Molecular,Models, Statistical,Models, Theoretical,Molecular Conformation,Photons,Protein Conformation,Protein Folding,Spectrophotometry,Spectrophotometry: methods,Time,Time Factors},
