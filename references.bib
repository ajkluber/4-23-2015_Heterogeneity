keywords = {Animals,Cell Movement,Cell Movement: physiology,Drosophila,Drosophila: embryology,Embryonic Development,Embryonic Development: physiology,Gene Expression Regulation, Developmental,Gene Expression Regulation, Developmental: genetic,Gene Regulatory Networks,Gene Regulatory Networks: genetics,Models, Biological,Proteins,Proteins: metabolism},
