month = apr,
number = {4},
pages = {985--94},
pmid = {9545386},
title = {{Contact order, transition state placement and the refolding rates of single domain proteins.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/9545386},
volume = {277},
year = {1998}
}
