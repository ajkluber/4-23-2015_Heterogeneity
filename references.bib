keywords = {Bacterial Proteins,Bacterial Proteins: chemistry,Biophysical Phenomena,Biophysics,Computer Simulation,Kinetics,Lac Repressors,Models, Molecular,Protein Folding,Repressor Proteins,Repressor Proteins: chemistry},
month = oct,
