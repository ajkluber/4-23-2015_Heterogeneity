journal = {Nat. Biotechnol.},
keywords = {Amino Acid Sequence,Computer Simulation,Genetic Variation,Genetic Variation: genetics,Models, Chemical,Models, Genetic,Models, Molecular,Molecular Sequence Data,Protein Conformation,Proteins,Proteins: chemistry,Proteins: genetics,Sequence Analysis, Protein,Sequence Analysis, Protein: methods},
