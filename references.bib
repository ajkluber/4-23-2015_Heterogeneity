journal = {Proc. Natl. Acad. Sci.},
keywords = {Algorithms,Animals,Biophysical Phenomena,Biophysics,Gene Expression,Models, Molecular,Models, Theoretical,Molecular Biology,Molecular Biology: methods,Stochastic Processes},
