pages = {87--92},
pmid = {15618401},
title = {{A speed limit for conformational change of an allosteric membrane protein.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=544059\&tool=pmcentrez\&rendertype=abstract},
volume = {102},
year = {2005}
}
