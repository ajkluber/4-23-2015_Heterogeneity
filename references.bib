pmid = {10860987},
title = {{Measuring the rate of intramolecular contact formation in polypeptides.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=16526\&tool=pmcentrez\&rendertype=abstract},
volume = {97},
year = {2000}
}
