keywords = {Cell Line, Tumor,Fluorescence Resonance Energy Transfer,Humans,Kinetics,Phosphoglycerate Kinase,Phosphoglycerate Kinase: chemistry,Phosphoglycerate Kinase: genetics,Phosphoglycerate Kinase: metabolism,Protein Folding},
