keywords = {Amino Acid Sequence,Conserved Sequence,Evolution, Molecular,Immunoglobulins,Immunoglobulins: chemistry,Kinetics,Models, Molecular,Models, Statistical,Molecular Sequence Data,Mutation,Protein Folding,Sequence Homology, Amino Acid,Solvents,Solvents: chemistry},
month = aug,
number = {1},
pages = {177--96},
pmid = {10438614},
title = {{Universally conserved positions in protein folds: reading evolutionary signals about stability, folding kinetics and function.}},
