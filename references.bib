pages = {5142--7},
pmid = {12702764},
title = {{The molecular basis for the chemical denaturation of proteins by urea.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=154312\&tool=pmcentrez\&rendertype=abstract},
volume = {100},
