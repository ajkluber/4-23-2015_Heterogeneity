issn = {0027-8424},
journal = {Proc. Natl. Acad. Sci.},
number = {1},
pages = {163--168},
pmid = {19966296},
title = {{Gatekeeper residues in the major curlin subunit modulate bacterial amyloid fiber biogenesis.}},
volume = {107},
year = {2010}
}
