month = feb,
number = {1},
pages = {42--9},
pmid = {15718132},
title = {{The family feud: do proteins with similar structures fold via the same pathway?}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15718132},
volume = {15},
year = {2005}
}
