issn = {1545-9993},
journal = {Nat. Struct. Mol. Biol.},
keywords = {Adenylate Kinase,Adenylate Kinase: chemistry,Adenylate Kinase: metabolism,Catalysis,Nuclear Magnetic Resonance, Biomolecular},
