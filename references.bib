@article{Yang2004,
abstract = {The same energy landscape principles associated with the folding of proteins into their monomeric conformations should also describe how these proteins oligomerize into domain-swapped conformations. We tested this hypothesis by using a simplified model for the epidermal growth factor receptor pathway substrate 8 src homology 3 domain protein, both of whose monomeric and domain-swapped structures have been solved. The model, which we call the symmetrized Gō-type model, incorporates only information regarding the monomeric conformation in an energy function for the dimer to predict the domain-swapped conformation. A striking preference for the correct domain-swapped structure was observed, indicating that overall monomer topology is a main determinant of the structure of domain-swapped dimers. Furthermore, we explore the free energy surface for domain swapping by using our model to characterize the mechanism of oligomerization.},
author = {Yang, Sichun and Cho, Samuel S and Levy, Yaakov and Cheung, Margaret S and Levine, Herbert and Wolynes, Peter G and Onuchic, Jos\'{e} N},
doi = {10.1073/pnas.0403724101},
