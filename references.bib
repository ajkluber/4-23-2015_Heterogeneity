journal = {Bioinformatics},
keywords = {Algorithms,Amino Acid Sequence,Amino Acids,Amino Acids: chemistry,Computational Biology,Computational Biology: methods,Databases, Protein,Phylogeny,Proteins,Proteins: chemistry,Sequence Alignment,Sequence Alignment: methods,Sequence Analysis, Protein},
