keywords = {Amino Acid Sequence,Ankyrin Repeat,Bacterial Proteins,Bacterial Proteins: chemistry,Halorhodospira halophila,Halorhodospira halophila: chemistry,Models, Molecular,Molecular Sequence Data,Protein Denaturation,Protein Folding,Receptors, Notch,Receptors, Notch: chemistry,Thermodynamics},
