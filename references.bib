keywords = {Algorithms,Biophysical Phenomena,Biophysics,Computer Simulation,Magnetic Resonance Spectroscopy,Models, Molecular,Monte Carlo Method,Protein Conformation,Protein Folding,Proteins,Proteins: chemistry,Protons,Temperature,Thermodynamics,Tryptophan,Tryptophan: chemistry,Water,Water: chemistry},
month = nov,
number = {23},
pages = {13280--5},
