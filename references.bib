issn = {1097-0134},
journal = {Proteins},
keywords = {Algorithms,Amino Acids,Amino Acids: chemistry,Animals,Cattle,Chickens,Databases, Protein,Magnetic Resonance Spectroscopy,Molecular Dynamics Simulation,Peptides,Peptides: chemistry,Protein Structure, Secondary,Proteins,Proteins: chemistry,Quantum Theory,Reproducibility of Results,Thermodynamics,Torsion, Mechanical},
month = jun,
