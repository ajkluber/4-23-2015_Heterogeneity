doi = {10.1016/j.bpj.2010.11.017},
file = {:home/alex/Work/Reading/All/Cheng\_Uzawa\_Plaxco\_Makarov\_Universality\_in\_the\_timescales\_of\_internal\_loop\_formation\_in\_unfolded\_proteins\_and\_single-stranded\_oligonucleotides\_2010.pdf:pdf},
issn = {1542-0086},
journal = {Biophys. J.},
keywords = {Computer Simulation,Kinetics,Models, Molecular,Oligonucleotides,Oligonucleotides: chemistry,Polymers,Polymers: chemistry,Protein Structure, Secondary,Protein Unfolding,Proteins,Proteins: chemistry,Proteins: metabolism,Time Factors},
month = dec,
number = {12},
pages = {3959--68},
pmid = {21156138},
