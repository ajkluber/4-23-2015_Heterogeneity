journal = {PLoS Comput. Biol.},
keywords = {Computer Simulation,Maltose-Binding Proteins,Maltose-Binding Proteins: chemistry,Maltose-Binding Proteins: ultrastructure,Models, Chemical,Models, Molecular,Protein Conformation,Protein Structure, Tertiary},
month = jan,
number = {4},
pages = {e1002471},
pmid = {22532792},
title = {{Exploration of multi-state conformational dynamics and underlying global functional landscape of maltose binding protein.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3330084\&tool=pmcentrez\&rendertype=abstract},
volume = {8},
year = {2012}
}
