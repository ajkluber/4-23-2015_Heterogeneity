journal = {Protein Sci.},
keywords = {computer algorithm,modular protein design,protein,protein folding,protein structure alignment},
pages = {1691--1698},
pmid = {1304898},
title = {{A database of protein structure families with common folding motifs.}},
volume = {1},
year = {1992}
}
