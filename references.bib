issn = {0022-2836},
journal = {J. Mol. Biol.},
keywords = {Amino Acid Sequence,Amino Acid Substitution,Calorimetry,Hydrogen Bonding,Kinetics,Models, Chemical,Mutagenesis, Site-Directed,Point Mutation,Protein Denaturation,Protein Folding,Recombinant Proteins,Recombinant Proteins: chemistry,Recombinant Proteins: metabolism,Ribonucleases,Ribonucleases: chemistry,Ribonucleases: metabolism,Thermodynamics,Urea,Urea: pharmacology},
