keywords = {Amino Acid Sequence,Animals,Chickens,Humans,Molecular Sequence Data,Nuclear Magnetic Resonance, Biomolecular,Protein Conformation,Protein Folding,Sequence Homology, Amino Acid,Solutions,Spectrin,Spectrin: chemistry},
month = oct,
number = {3},
pages = {740--51},
pmid = {9356261},
title = {{Solution structure of the spectrin repeat: a left-handed antiparallel triple-helical coiled-coil.}},
