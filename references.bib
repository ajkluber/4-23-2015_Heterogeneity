file = {:home/alex/Work/Reading/All/Kolomeisky\_Stukalin\_Popov\_Understanding\_mechnochemical\_coupling\_in\_kinesins\_using\_first-passage-time\_processes\_2005.pdf:pdf},
issn = {15393755},
journal = {Phys. Rev. E - Stat. Nonlinear, Soft Matter Phys.},
pages = {1--8},
