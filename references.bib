issn = {1096-0384},
journal = {Arch. Biochem. Biophys.},
keywords = {Protein Conformation,Protein Denaturation,Proteins,Proteins: chemistry,Spectrum Analysis,Spectrum Analysis: methods},
month = mar,
number = {1-2},
pages = {4--13},
pmid = {23142683},
title = {{The stability of 2-state, 3-state and more-state proteins from simple spectroscopic techniques... plus the structure of the equilibrium intermediates at the same time.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23142683},
volume = {531},
year = {2013}
}
