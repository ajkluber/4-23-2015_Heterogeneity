@article{Cohen2014,
abstract = {It is common to denature proteins by high temperatures or by adding guanidinium chloride (GdmCl). However, the physical mechanism of denaturation is not well understood. Based on extensive experimental data, we developed a thermodynamic binding-polynomial model for the process of transferring nonpolar amino acids from water into GdmCl solutions, as a function of temperature and GdmCl concentration. To mimic nonpolar amino acids, we utilized the model compound, N-acetyl-tryptophanamide (NATA). We find that all nonpolar amino acids behave like NATA, with a scale factor proportional to surface area. Our model with three thermodynamic parameters fully captures the nonlinear dependencies on both temperature and GdmCl concentration: binding the first guanidinium ion to NATA has favorable entropy and unfavorable enthalpy of desolvation ($\Delta$S = + 11.7 cal/mol, $\Delta$H = + 3.9 kcal/mol), while cooperativity of binding a second guanidinium has a small contribution (K = 0.032 ± 0.003). This model may be useful for better understanding of protein denaturation by temperature and GdmCl.},
author = {Cohen, Paul and Dill, Ken a and Jaswal, Sheila S},
doi = {10.1021/jp506379r},
file = {:home/alex/Work/Reading/All/Cohen\_Dill\_Jaswal\_Modeling\_the\_solvation\_of\_nonpolar\_amino\_acids\_in\_Guanidinium\_Chloride\_solutions\_2014.pdf:pdf},
issn = {1520-5207},
journal = {J. Phys. Chem. B},
month = aug,
pmid = {25141127},
title = {{Modeling the Solvation of Nonpolar Amino Acids in Guanidinium Chloride Solutions.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25141127},
year = {2014}
}
