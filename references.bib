issn = {1476-4687},
journal = {Nature},
keywords = {Crystallography, X-Ray,Cyanobacteria,Cyanobacteria: chemistry,Hydrophobic and Hydrophilic Interactions,Ion Channel Gating,Ion Channels,Ion Channels: chemistry,Ion Channels: metabolism,Ligands,Models, Molecular,Pectobacterium chrysanthemi,Pectobacterium chrysanthemi: chemistry,Protein Structure, Quaternary,Protein Subunits,Protein Subunits: chemistry,Protein Subunits: metabolism},
month = jan,
number = {7225},
