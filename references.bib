issn = {0021-9606},
journal = {J. Chem. Phys.},
keywords = {Algorithms,Computer Simulation,Computing Methodologies,Kinetics,Markov Chains,Microfilament Proteins,Microfilament Proteins: chemistry,Microfilament Proteins: ultrastructure,Models, Chemical,Models, Molecular,Models, Statistical,Protein Conformation,Protein Folding},
month = apr,
number = {16},
