issn = {1463-9084},
journal = {Phys. Chem. Chem. Phys.},
keywords = {Cluster Analysis,Molecular Dynamics Simulation,Peptides,Peptides: chemistry,Protein Structure, Tertiary,Proteins,Proteins: chemistry},
month = jun,
