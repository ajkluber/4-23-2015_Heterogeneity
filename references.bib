doi = {10.1006/jmbi.2000.4378},
file = {:home/alex/Work/Reading/All/Cota\_Steward\_Fowler\_Clarke\_The\_folding\_nucleus\_of\_a\_fibronectin\_type\_III\_domain\_is\_composed\_of\_core\_residues\_of\_the\_immunoglobulin-like\_fold\_2001.pdf:pdf},
