journal = {J. Mol. Biol.},
keywords = {Animals,Computer Simulation,Humans,Microscopy, Atomic Force,Models, Molecular,Protein Structure, Secondary,Protein Structure, Tertiary,Sequence Analysis, Protein,Spectrin,Spectrin: chemistry,Time Factors,Water,Water: metabolism},
month = jun,
number = {3},
pages = {638--47},
pmid = {15896349},
title = {{Unfolding a linker between helical repeats.}},
