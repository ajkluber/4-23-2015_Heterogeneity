file = {:home/alex/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Resenberger et al. - 2011 - The cellular prion protein mediates neurotoxic signalling of $\beta$-sheet-rich conformers independent of prion replication.pdf:pdf},
