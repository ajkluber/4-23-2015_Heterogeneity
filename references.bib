keywords = {Brain Diseases,Brain Diseases: etiology,Computer Simulation,Humans,Huntington Disease,Huntington Disease: etiology,Models, Molecular,Nanotubes,Peptides,Peptides: chemistry,Protein Conformation,Protein Structure, Secondary},
